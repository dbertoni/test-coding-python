{
    "name": "Python FastAPI",
    "build": {
      "context": "..",
      "dockerfile": "Dockerfile"
    },
    "forwardPorts": [8000],
    "overrideCommand": false,
    "customizations": {
      "vscode": {
        "settings": {
          "[python]": {
            "editor.formatOnType": true,
            "editor.formatOnSave": true
          }
        },
        "extensions": [
          "ms-python.python",
          "GitHub.copilot-nightly",
          "ms-azuretools.vscode-docker"
        ]
      }
    }
  }
  